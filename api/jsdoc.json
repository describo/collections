{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".(js)$"
    },
    "sourceType": "module",
    "plugins": ["jsdoc-route-plugin"],
    "opts": {
        "readme": "../README.md",
        "recurse": true,
        "destination": "../docs",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "light",
            "include_css": ["./src/docs/theme.css"]
        }
    }
}
